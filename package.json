    "supabase:db:new-migration": "supabase migration new",
    "supabase:functions:serve": "supabase functions serve",
    "supabase:functions:deploy": "supabase functions deploy",
    "db:seed": "supabase db reset --seed",
    "db:backup": "supabase db dump --data-only --file backup_$(date +%Y%m%d_%H%M%S).sql",
    "deploy:staging": "npm run build:dev && supabase db push --linked",
    "deploy:prod": "npm run build:prod && supabase db push --linked",
    "clean": "rm -rf dist node_modules/.cache",
    "clean:all": "rm -rf dist node_modules .supabase .temp",
    "install:clean": "npm run clean:all && npm install",
    "dev:full": "concurrently \"npm run dev\" \"npm run supabase:start\" \"npm run supabase:functions:serve\"",
    "dev:supabase": "concurrently \"npm run supabase:start\" \"npm run supabase:functions:serve\"",
    "check:all": "npm run lint && npm run type-check && npm run format:check",
    "pre-commit": "npm run lint:fix && npm run type-check && npm run format"
